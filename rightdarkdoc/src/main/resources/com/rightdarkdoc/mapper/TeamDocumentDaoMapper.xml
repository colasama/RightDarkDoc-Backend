<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rightdarkdoc.dao.TeamDocumentDao">

    <select id="findAllTeamDocuments" resultType="Integer">
        select docid from Team_Document where teamid = #{teamid}
    </select>

    <insert id="createNewTeamDocument" parameterType="Integer">
        insert into Team_Document(teamid, docid) values (#{teamid}, #{docid})
    </insert>

</mapper>